{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017"
  },
  "compileOnSave": false,
  "exclude": [
    
    "wwwroot",
    "node_modules"
  ],
  "include": [
    "src/**/*.ts",
    "*.ts",
    "file.ts",
    ".eslintrc.js"
  ]
}